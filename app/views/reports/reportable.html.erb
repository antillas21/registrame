 {
"sEcho": <%=h @sEcho %>,
"iTotalRecords": <%= @iTotalRecords %>,
"iTotalDisplayRecords": <%= @iTotalDisplayRecords %>,
"aaData" : [ 
<% @users.each do |user| %>
   [   "<%= user.registration.name %>",
	"<%= user.full_name %>",
	"<%= user.email %>",
	"<%= user.job.blank? ? '--' : user.job %>",
	"<%= user.department.nil? ? '--' : user.department.name %>",
	"<%= user.company.nil? ? '--' : user.company.name %>",
	"<%= user.company.nil? || user.company.address1.blank? ? '--' : user.company.address1 %> <%= user.company.nil? || user.company.address2.blank? ? '--' : user.company.address2 %>",
	"<%= user.company.nil? || user.company.city.blank? ? '--' : user.company.city %>",
	"<%= user.company.nil? || user.company.state.nil? || user.company.state.name.blank? ? '--' : user.company.state.name %>",
	"<%= user.company.nil? || user.company.zip.blank? ? '--' : user.company.zip %>",
	"<%= user.company.nil? || user.company.country.nil? || user.company.country.name.blank? ? '--' : user.company.country.name %>",
	"<%= user.company.nil? || user.company.phone.blank? ? '--' : user.company.phone %>",
	"<%= user.company.nil? || user.company.website.blank? ? '--' : user.company.website %>",
	"<%= user.company.nil? || user.company.sector.nil? || user.company.sector.name.blank? ? '--' : user.company.sector.name %>",
	"<% user.interests.each do |interest| %><%= interest.name %><%= ', ' unless interest == user.interests.last %><% end %>",
	"<% user.promos.each do |promo| %><%= promo.name %><%= ', ' unless promo == user.promos.last %><% end %>"
   <%= "]," unless user == @users.last %>
<% end %>
   ]
]
}
